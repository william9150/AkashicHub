{
  "name": "akashichub-ui",
  "version": "1.0.0",
  "description": "AkashicHub Frontend - IT Resource Management System",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:development": "vite --mode development",
    "dev:production": "vite --mode production",
    "dev:windows": "vite --mode development --host 0.0.0.0",
    "dev:test": "vite --config vite.config.test.ts",
    "build": "vue-tsc && vite build",
    "build:development": "vue-tsc && vite build --mode development",
    "build:production": "vue-tsc && vite build --mode production",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "diagnose": "node diagnose.js",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "vue-tsc --noEmit",
    "clean": "rm -rf dist node_modules/.cache"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.0",
    "axios": "^1.6.0",
    "dayjs": "^1.11.0",
    "echarts": "^5.4.0",
    "element-plus": "^2.4.0",
    "lodash-es": "^4.17.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.0",
    "vue": "^3.4.0",
    "vue-echarts": "^6.6.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^20.10.0",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.0",
    "cypress": "^13.6.0",
    "date-fns": "^4.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^23.0.0",
    "prettier": "^3.1.0",
    "sass": "^1.69.0",
    "typescript": "^5.3.0",
    "unplugin-auto-import": "^0.17.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "vue-tsc": "^1.8.0"
  },
  "keywords": [
    "vue3",
    "typescript",
    "vite",
    "element-plus",
    "pinia",
    "resource-management",
    "enterprise"
  ],
  "author": "AkashicHub Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
